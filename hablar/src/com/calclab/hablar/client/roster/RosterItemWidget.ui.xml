<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="RosterWidget.css" />

	<ui:style field="icons">
		@sprite .userAdd {
			gwt-image: 'buddy';
		}
	</ui:style>

	<ui:image field="buddy" src="buddy.gif"></ui:image>
	<g:HTMLPanel styleName="{style.item}">
		<span class="{style.icon} {icons.userAdd}"></span>
		<g:Label styleName="{style.name}" ui:field="name" />
	</g:HTMLPanel>
</ui:UiBinder> 