<module>
  <!-- other inherited modules, such as com.google.gwt.user.User -->
  <inherits name="com.google.gwt.user.User" />

  <inherits name="com.calclab.emite.Emite" />
  <inherits name="com.allen_sauer.gwt.log.gwt-log" />
  <inherits name='com.allen_sauer.gwt.voices.gwt-voices'/>
  <inherits name="org.ourproject.kune.platf.Kune-Platform" />

  <public path="public" />
  <entry-point
    class="com.calclab.emiteuimodule.client.EmiteUIModuleEntryPoint" />

  <extend-property name="log_level" values="DEBUG,INFO,OFF" />
  <set-property name="log_DivLogger" value="DISABLED" />
  <set-property name="log_FirebugLogger" value="ENABLED" />

  <inherits name='com.gwtext.GwtExt' />
  <script src="js/ext/adapter/ext/ext-base.js" />
  <script src="js/ext/ext-all.js" />

  <stylesheet src="emite.css" />

</module>
